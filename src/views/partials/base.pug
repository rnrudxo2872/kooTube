include ../mixins/message

doctype html
html(lang="ko")
    include head
    body 
        if messages.error
            +message("error",messages.error) 
        if messages.info
            +message("info",messages.info) 
        if messages.success
            +message("success",messages.success) 
        include header
        main
            block content
                h3 잘못된 페이지
        include footer
    script(src="/static/js/main.js") 
    block scripts